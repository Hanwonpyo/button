package button;

import javax.swing.JButton;

public class buttonFactory {
	
	Button button;
	public Button makeButton(int i) {
		if(i==0)
			button=new Button("1");
		if(i==1)
			button=new Button("2");
		if(i==2)
			button=new Button("3");
		if(i==3)
			button=new Button("4");
		if(i==4)
			button=new Button("allo");
		if(i==5)
			button=new Button("allx");
		
		return button;
	}
}
