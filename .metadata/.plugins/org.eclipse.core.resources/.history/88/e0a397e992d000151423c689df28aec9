package button;

import javax.swing.JButton;

public class Button extends JButton implements Observer{

	@Override
	public void notifyObserver(int n) {
		if(n==4)
			this.setEnabled(true);
		else if(n==5)
			this.setEnabled(false);
		else {
			int temp=Integer.parseInt(this.getName());
			if(temp%2)==n)
				this.setEnabled(false);
			else
				this.setEnabled(true);
		}		
	}		
}
